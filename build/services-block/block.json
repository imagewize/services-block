{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "imagewize/services-block",
  "version": "0.1.0",
  "title": "Services Block",
  "category": "widgets",
  "icon": "editor-ul",
  "description": "Services section with heading, intro and service items",
  "supports": {
    "html": false,
    "color": {
      "background": true,
      "text": true,
      "gradients": true
    },
    "spacing": {
      "padding": true
    }
  },
  "textdomain": "services-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "style": {
      "type": "object",
      "default": {
        "color": {
          "background": "var(--wp--preset--color--slate-50)"
        }
      }
    }
  }
}